{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfcicddevfornewdmo"
		},
		"Ls_01_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "mynewcicds"
		},
		"azurecicd_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://mycicdsetkey.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfcicddevfornewdmo_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"azurecicd_properties_typeProperties_baseUrl": {
						"value": "[parameters('azurecicd_properties_typeProperties_baseUrl')]"
					},
					"Ls_01_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('Ls_01_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}